{
  "name": "devex",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "test:vitest:headed": "HEADED=true vitest run",
    "test:vitest:headed:watch": "HEADED=true vitest",
    "test:vitest:headless": "vitest run"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "devextreme": "^25.2.2-beta",
    "devextreme-angular": "^25.2.2-beta",
    "devextreme-schematics": "^1.12.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "@types/node": "^25.0.1",
    "@vitest/browser-playwright": "^4.0.15",
    "devextreme-cli": "latest",
    "devextreme-themebuilder": "25.1.6",
    "jsdom": "^27.1.0",
    "playwright": "^1.57.0",
    "sass-embedded": "1.66.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
